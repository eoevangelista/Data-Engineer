{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-strider"
		},
		"AzureStride_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStride'"
		},
		"stride_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'stride'"
		},
		"trg_stride_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/c1c220f9-ee03-4e05-8c17-d6252e384214/resourceGroups/estudos/providers/Microsoft.Storage/storageAccounts/estudosa52e"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-strider_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStride_connectionString": {
						"value": "[parameters('AzureStride_connectionString')]"
					},
					"stride_connectionString": {
						"value": "[parameters('stride_connectionString')]"
					},
					"trg_stride_properties_typeProperties_scope": {
						"value": "[parameters('trg_stride_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}